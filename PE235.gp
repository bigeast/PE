f(r) = {
	t = r^5000;
	-((300-5000)*r*t+(5000-299)*t-300*r+299)/(1-r)^2-2*10^11
}
a = 1.002
b = 1.003
for(N=1,30,mid=(a+b)/2;t=f(mid);if(t<0,a=mid,b=mid);print(t))
